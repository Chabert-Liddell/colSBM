X = seq_along(my_files),
FUN = function(i) {
df <- read.csv(file = paste0("~/Documents/ecological_network/data/food_web/",
my_files[i]), header = TRUE, row.names = 1)
A <- as.matrix(df)
return(list(
net = A,
nr = nrow(A),
nc = ncol(A),
dens = mean(A),
id = stringr::str_sub(my_files[i], 1, -5))
)
}
)
